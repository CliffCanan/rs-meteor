body
  font-family 'Raleway', Helvetica, sans-serif
  font-size 14.5px
  background #fff
  color #333
  margin 0
  padding 0
  width 100%

main
  margin 70px 0 0

.content-wrapper
  padding 10px
  @media screen and (min-width: 768px)
    padding 20px

.p-relative
  position relative

a
  color #4588fa
  
  > .fa
    margin-left 6px
	margin-right 6px

button > .fa
  margin-right 6px

.btn .fa
  margin-right 4px

@media screen and (min-width: 768px)
  .btn.dropdown-toggle .caret
    float right
    position relative
    top 8px

a.carousel-control .fa
  margin-left 0
  margin-right 0

.modal-body,
.modal-body p
  line-height 1.4
@media screen and (max-width: 768px)
  .modal-title
    text-indent -13px
    padding-left 25px
  .modal-body .row .col-xs-6:first-child
    padding-right 5px
  .modal-body .row .col-xs-6:last-child
    padding-left 5px
  .modal-body .row .col-xs-6 .input-group .form-control
    padding 5px

