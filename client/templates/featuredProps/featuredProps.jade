template(name="featuredProps")

  section.featured-props-wrap.clearfix
    .container-fluid.p-b-25

      h2.main-section-title Featured Listings

      .col-xs-12
        .row

          .col-sm-6.col-md-4.col-lg-3
            .card-container
              .card
                .front
                  .cover
                    .ribbon
                      span 2 Months Free!
                    img(src="/cfs/files/images/TKaRmFfgc9y3TxLeT/2img_image 2.jpg?store=thumbs", itemprop="image", alt="2040 Market Street")
                  .content
                    .main
                      h3.name 2040 Market Street
                      p.price $1595 / mo.
                      h5
                        i.fa.fa-bed.fa-fw
                        | 1-3 Bedrooms
                      h5
                        img.bath-icon(src="/images/bath.svg")
                        | 0.5-2 Bathrooms
                      h5
                        i.fa.fa-calendar.fa-fw
                        | Available Feb 1

                    .footer
                      .note.special 2 Months Free

                .back
                  .header
                    .neighborhood Center City West
                  .content
                    .main
                      h4.text-center Amenitites
                      ul.fa-ul
                        li
                          i.fa.fa-li.fa-check-circle
                          | In-unit washer & dryer
                        li
                          i.fa.fa-li.fa-check-circle
                          | Stainless steel appliances
                        li
                          i.fa.fa-li.fa-check-circle
                          | Pet-friendly
                        li
                          i.fa.fa-li.fa-check-circle
                          | Doorman
                        li
                          i.fa.fa-li.fa-check-circle
                          | Valet parking or private garage
                      h4.text-center Neighborhood
                      p Blocks of restaurants, entertainment, shopping, culture and parks are right at your fingertips.
                  .footer
                    .btn-group.btn-group-justified
                      a.btn(href="/city/philadelphia/center-city-west/2040-market-st", title="Click to see 2040 Market St!") View
                      a.btn.check-availability Check Availability


