                    //-  span.building-main-prop-text
                    //-    | Not Furnished
                each complexFields
                  li.building-main-prop.col-lg-6.col-md-4.col-sm-6.col-xs-12(data-toggle="tooltip", data-placement="top", title="{{commentValue}}")
                    i.fa(class="{{iconClass}}")
                    span.building-main-prop-text
                      | {{text}}
                    if commentValue
